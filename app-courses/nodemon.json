{
    "watch": [
        "src"
    ],
    "ext": "ts",
    "env": {
        "NODE_ENV": "development"
    },
    "ignore": [
        "node_modules",
        "dist",
        "declare"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "verbose": true,
    "restartable": "rs"
}